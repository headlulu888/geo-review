        margin-left: 15px;
    }

    .c-yell {
        position:absolute;
        display:block;
        left: 40px;
        top: 40px;
        width: 380px;
        height: 530px;
        overflow: hidden;
        border-radius: 15px;
        background-color: white;
        z-index: 1;

    }



    .c-yell_hidden {
        display:none;
    }

    .c-yell__title-bar {
        background-color: #ff8663;
        display: flex;
        justify-content: space-between;
        align-items: center;
    }

    .c-yell__address {
        color: white;
        font-size: 12px;
        margin-left: 10px;
    }

    .c-yell__close-button {
        margin-left: auto;
        padding: 15px;
        text-decoration: none;
        font-size: 30px;
        height: 20px;
        width: 20px;
        color: #AAAAAA;
    }

    .c-yell__close-icon {
        height: 16px;
        width: 16px;
        -webkit-mask: url('./fonts/plus.svg');
        mask: url('./fonts/plus.svg');
        -webkit-mask-size: cover;
        mask-size: cover;
        background-color: #FFFFFF;
        transform: rotate3d(0, 0, 1, 45deg);
    }

    .c-form__wrapper {
        padding: 15px;
    }

    .c-comments {
        height: 120px;
        overflow-y: auto;
        overflow-x: hidden;
        border-bottom: solid thin #CCCCCC;
        margin-bottom: 10px;
