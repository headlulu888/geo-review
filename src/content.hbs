<style>
    body {
        line-height: 1.42;
    }

    li {
        list-style: none;
    }

    ul {
        padding: 0;
        margin: 0;
    }

    .l-container {
        display: flex;
        width: 100vw;
        height: 100vh;
        justify-content: center;
        justify-items: center;
        position:relative;
    }

    .fa-map-marker-alt {
        width: 30px;
