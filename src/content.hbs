        color: #ff8663;
        text-transform: uppercase;
        font-size: 18px;
        font-weight: 600;
        margin-bottom: 5px;
    }

    .c-form__input {
        height: 30px;
        border-radius: 15px;
        border: solid thin #cccccc;
        margin: 5px auto;
        width: 335px;
        padding-left: 15px;
    }

    .c-form__input_textarea {
        height: auto;
        width: 330px;
    }

